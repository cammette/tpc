<configuration xmlns="urn:hornetq"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:hornetq /schema/hornetq-jms.xsd">
	<!--the connection factory used by the example -->
	<connection-factory name="ConnectionFactory">
		<connectors>
			<connector-ref connector-name="in-vm" />
		</connectors>
		<entries>
			<entry name="ConnectionFactory" />
		</entries>
		<connection-ttl>-1</connection-ttl>
		<block-on-durable-send>true</block-on-durable-send>
		<consumer-window-size>100</consumer-window-size>
		<producer-max-rate>-1</producer-max-rate>
	</connection-factory>

	<!--the queue used by the example -->
	<queue name="travelskyQueue">
		<entry name="/queue/travelskyQueue" />
	</queue>
	
    <queue name="travelskyNotifyQueue">
		<entry name="/queue/travelskyNotifyQueue" />
	</queue>
	
	<queue name="travelskyReplyQueue">
		<entry name="/queue/travelskyReplyQueue" />
	</queue>

	<queue name="deadLetterQueue">
		<entry name="/queue/deadLetterQueue" />
	</queue>
	
	<queue name="travelskyGroupQueue">
		<entry name="/queue/travelskyGroupQueue" />
	</queue>
	
	
	<queue name="travelskyGroupReplyQueue">
		<entry name="/queue/travelskyGroupReplyQueue" />
	</queue>
	<queue name="test">
		<entry name="/queue/test" />
		<durable>false</durable>
	</queue>


</configuration>
